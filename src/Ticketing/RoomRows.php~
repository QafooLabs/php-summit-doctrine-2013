<?php

namespace Ticketing;

use Doctrine\ORM\Mapping as ORM;

/**
 * RoomRows
 */
class RoomRows
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var \Ticketing\Room
     */
    private $room;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set room
     *
     * @param \Ticketing\Room $room
     * @return RoomRows
     */
    public function setRoom(\Ticketing\Room $room = null)
    {
        $this->room = $room;

        return $this;
    }

    /**
     * Get room
     *
     * @return \Ticketing\Room 
     */
    public function getRoom()
    {
        return $this->room;
    }
}
